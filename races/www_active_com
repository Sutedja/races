  var initRec=setTimeout("(function (){var b,e=document.createElement('script'),a3igVersion='1.0.0.1',a3igHost='https://ig.active.com',cb=(new Date()).getTime() + '_' + Math.random();e.async=true;e.type='text/javascript';e.src=a3igHost+'/ig.loader.js?'+a3igVersion+'&'+cb;if(document.getElementsByTagName('script').length){b=document.getElementsByTagName('script')[0].parentNode;}else{b=document.getElementsByTagName('body')[0];}b.appendChild(e);}());",1);

  $('.search-item-title .title a').on('mousedown', function(e) {
    var data = {
      asset_id: $(this).data('asset-id'),
      result_position: $(this).parents('.search-item.activity').data('result-index'),
      page_number: a3ig_config.pageIndex,
      session_id: a3ig_config.session_id,
      url: a3ig_config.url
    }

    _igq.push({"eventType":"assetClick","eventBody":data});
    if (a3ig != null) {
      a3ig.update();
    }
  })

  $('#register-now-button').on('mousedown', function(e) {
    var data = {
      asset_id: $('meta[property="assetId"]').attr('content'),
      target: $(this).attr('href'),
      referrer: document.referrer
    }

    _igq.push({"eventType":"regClick","eventBody":data});
    if (a3ig != null) {
      a3ig.update();
    }
  })
</script>


